FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    "(BC2)"
    {
        type            boundaryCorrectedFixedValue;
        value           uniform 0;
    }
    "(BC1)"
    {
        type            calculated;
        value           uniform 0;
    }

}


